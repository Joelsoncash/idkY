{
  "data_feed": {
    "type": "CSV",
    "file_path": "data/historical_data.csv",
    "timeframe": "1D",
    "datetime_format": "%Y-%m-%d"
  },
  "strategy": {
    "name": "SampleStrategy",
    "params": {
      "sma_period": 20,
      "stop_loss": 0.02,
      "take_profit": 0.05
